<testscenario xmlns="www.pushtotest.com/tm5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="www.pushtotest.com/XSD/testscenario.xsd" version="2.0">
  <basics>
    <name>tc_C3Seller_QA_Pef</name>
    <defaultdirectory>{scenario}</defaultdirectory>
    <basicName>Ravendra</basicName>
    <creator>Ravendra</creator>
    <links/>
    <versions>1.0</versions>
    <categorization>C3 Seller QA web</categorization>
  </basics>
  <resources>
    <resource name="ts_Seller_LogIn.ds" type="ds" path="{scenario}/ts_Seller_LogIn.ds" cache="false"/>
    <resource name="ts_Seller_LotDisplay.ds" type="ds" path="{scenario}/ts_Seller_LotDisplay.ds" cache="false"/>
    <resource name="ts_Seller_OnSaleNow.ds" type="ds" path="{scenario}/ts_Seller_OnSaleNow.ds" cache="false"/>
    <resource name="ts_Seller_BidApproval.ds" type="ds" path="{scenario}/ts_Seller_BidApproval.ds" cache="false"/>
    <resource name="ts_Seller_Proquote.ds" type="ds" path="{scenario}/ts_Seller_Proquote.ds" cache="false"/>
    <resource type="data" path="REPOSITORY/C3Seller_perf/testdata/lync_users.csv" cache="false"/>
  </resources>
  <testnodes>
    <cloud name="amazaon_rc3" type="EC2" config="SERVER/home/ubuntu/Appvance/EC2_R3_Config.cloud" amount="2" instantiate="All" terminate="All" reboot="After"/>
  </testnodes>
  <bundles>
    <bundle name="hibernate" version="3.0"/>
    <bundle name="mysql" version="5.0"/>
    <bundle name="sahi" version="sahi_pro"/>
    <bundle name="ds" version="2.0"/>
    <bundle name="seleniumrc" version="2.45"/>
    <bundle name="htmlunit" version="2.15.2648"/>
  </bundles>
  <datasources>
    <dpl name="LyncUsers" type="HashDPL" langtype="java" method="setup">
      <argument name="Remote path of the file" dpl="Resources" method="getResourceByName">
        <argument name="file" value="lync_users.csv"/>
      </argument>
    </dpl>
  </datasources>
  <options>
    <options type="apc">
      <opt name="SleepTime" value="0"/>
      <opt name="testnode_concurrent_users" value="0"/>
      <opt name="global_concurrent_users" value="0"/>
      <opt name="SleepTime" value="0"/>
      <opt name="RampUpUsers" value="0"/>
      <opt name="RampUpSeconds" value="0"/>
      <opt name="RampDownUsers" value="0"/>
      <opt name="RampDownSeconds" value="0"/>
      <opt name="ThinkTimeMin" value="0"/>
      <opt name="ThinkTimeMax" value="0"/>
      <opt name="MinSleepTime" value="0"/>
      <opt name="MaxSleepTime" value="0"/>
      <opt name="MinIntervalTime" value="0"/>
      <opt name="MaxIntervalTime" value="0"/>
      <opt name="PercentageLogsSaved" value="0"/>
      <opt name="ThinkTime" value="0"/>
      <opt name="IntervalTime" value="0"/>
      <opt name="DynamicIpAddress" value="false"/>
      <opt name="EnableSavingLogs" value="true"/>
      <opt name="AlwaysSaveLogOnFailure" value="true"/>
      <opt name="AddThinkTimeToStep" value="false"/>
    </options>
    <options type="selenium">
      <opt name="loglevel" value="INFO"/>
      <opt name="throwExceptionOnScriptError" value="false"/>
      <opt name="throwExceptionOnFailingStatusCode" value="false"/>
    </options>
    <options type="sahi">
      <opt name="clearCache" value="true"/>
      <opt name="takeSnapshot" value="false"/>
    </options>
    <options type="seleniumrc">
      <opt name="listenport" value="6543"/>
      <opt name="port" value="5555"/>
    </options>
  </options>
  <templates>
    <controller name="tc_seller_qa_perf">
      <round name="tc_seller_qa_perf" totalusers="100" time="30" testtype="users" totaliterations="200"/>
    </controller>
    <testcase name="LogIn" expand="yes">
      <test>
        <run name="LogIn" testclass="sahi" method="firefox-xvfb" langtype="ds" resource="ts_Seller_LogIn.ds">
          <argument name="LyncUsers" method="getNextData" dpl="LyncUsers"/>
        </run>
      </test>
    </testcase>
    <testcase name="LotDisplay" expand="yes">
      <test>
        <run name="LotDisplay" testclass="sahi" method="firefox-xvfb" langtype="ds" resource="ts_Seller_LotDisplay.ds"/>
      </test>
    </testcase>
    <testcase name="OnSaleNow" expand="yes">
      <test>
        <run name="OnSaleNow" testclass="sahi" method="firefox-xvfb" langtype="ds" resource="ts_Seller_OnSaleNow.ds"/>
      </test>
    </testcase>
    <testcase name="BidApproval" expand="yes">
      <test>
        <run name="BidApproval" testclass="sahi" method="firefox-xvfb" langtype="ds" resource="ts_Seller_BidApproval.ds"/>
      </test>
    </testcase>
    <testcase name="Proquote" expand="yes" optionState="true" thinktimeState="true">
      <test>
        <run name="Proquote" testclass="sahi" method="firefox-xvfb" langtype="ds" resource="ts_Seller_Proquote.ds"/>
      </test>
    </testcase>
    <assertsuite xmlns="www.pushtotest.com/tm5" name="Optional"/>
    <storesuite xmlns="www.pushtotest.com/tm5" name="Optional"/>
  </templates>
  <monitoring enablednodes="true" enabledtarget="false"/>
  <execution type="loadtest">
    <iteration controller="tc_seller_qa_perf" name="tc_seller_qa_perf">
      <testcase name="LogIn" users="20"/>
      <testcase name="LotDisplay" users="20"/>
      <testcase name="OnSaleNow" users="20"/>
      <testcase name="BidApproval" users="20"/>
      <testcase name="Proquote" users="5">
        <options>
          <opt name="RampUpUsers" value="0"/>
          <opt name="RampUpSeconds" value="0"/>
          <opt name="MaxRampUpTime" value="0"/>
          <opt name="RampDownUsers" value="0"/>
          <opt name="RampDownSeconds" value="0"/>
          <opt name="MaxRampDownTime" value="0"/>
          <opt name="InitialRampUpDelay" value="0"/>
          <opt name="IntervalTime" value="0"/>
          <opt name="MinIntervalTime" value="0"/>
          <opt name="MaxIntervalTime" value="0"/>
          <opt name="SleepTime" value="0"/>
          <opt name="MinSleepTime" value="0"/>
          <opt name="MaxSleepTime" value="0"/>
          <opt name="ThinkTimeMin" value="10000"/>
          <opt name="ThinkTimeMax" value="15000"/>
        </options>
      </testcase>
    </iteration>
  </execution>
  <logs>
    <log type="database">
      <opt name="hibernate.connection.username" value="apc"/>
      <opt name="hibernate.connection.password" value="apc"/>
      <opt name="hibernate.connection.url" value="jdbc:mysql://ec2-52-91-69-250.compute-1.amazonaws.com:3306/apc"/>
      <opt name="hibernate.default_schema" value="apc"/>
      <opt name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
    </log>
  </logs>
  <integrations>
    <integration type="RQM">
      <opt name="Enabled" value="false"/>
    </integration>
  </integrations>
  <notifications/>
</testscenario>
